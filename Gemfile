source 'https://rubygems.org'

# github-pages gem uses jekyll 3.6.2 which is not compatible with 3.7.0
# gem 'github-pages', group: :jekyll_plugins
gem "jekyll", "3.7.0"
gem "html-proofer"

group :jekyll_plugins do
  gem 'jekyll-sitemap'
  gem 'jekyll-seo-tag'
  gem 'jekyll-feed'
  gem 'replace_regex'
end
